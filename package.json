{
  "name": "angular2-bootstrap-pagination",
  "version": "1.0.5",
  "description": "angular2-bootstrap-pagination is angular2 directive. It will generate pagination or this directive must need boostrap package",
  "main": "index.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajan-g/angular2-bootstrap-pagination.git"
  },
  "dependencies": {
    "@angular/common": "^2.0.0-rc.4",
    "@angular/compiler": "^2.0.0-rc.4",
    "@angular/core": "^2.0.0-rc.4",
    "@angular/platform-browser": "^2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "^2.0.0-rc.4",
    "@angular/upgrade": "^2.0.0-rc.4",
    "bootstrap": "^3.3.6"
  },
  "devDependencies": {
   "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  },
  "keywords": [
    "angular2-bootstrap-pagination",
    "ng2-bootstrap-pagination",
     "ng2-pagination",
    "bootstrap-pagination"
  ],
  "author": "Rajan G",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rajan-g/angular2-bootstrap-pagination.git/issues"
  },
  "homepage": "https://github.com/rajan-g/angular2-bootstrap-pagination.git#readme"
}
