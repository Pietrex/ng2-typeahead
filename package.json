{
  "name": "angular2-bootstrap-pagination",
  "version": "1.0.1",
  "description": "angular2-bootstrap-pagination is angular2 directive. It will generate pagination or this directive must need boostrap package",
  "main": "index.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajan-g/angular2-letter-avatar.git"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.17",
    "bootstrap": "^3.3.6",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.26",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  },
  "keywords": [
    "angular2-bootstrap-pagination",
    "ng2-bootstrap-pagination",
     "ng2-pagination",
    "bootstrap-pagination"
  ],
  "author": "Rajan G",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rajan-g/angular2-letter-avatar/issues"
  },
  "homepage": "https://github.com/rajan-g/angular2-letter-avatar#readme"
}
